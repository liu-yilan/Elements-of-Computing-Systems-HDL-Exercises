/** 
 * Multiplexor:
 * if (sel = 0) out = a, else out = b
 */
CHIP Mux {
    IN a, b, sel;
    OUT out;

    PARTS:
    Not(in=sel, out=notsel); 
    And(a=notsel, b=a, out=res1); 
    And(a=sel, b=b, out=res2); 
    Or(a=res1, b=res2, out=out);
}